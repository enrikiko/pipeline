pipeline {
   agent any

   stages {
      stage('Deploy DNS') {
         steps {
            echo "Running ${env.BUILD_ID} TAG:${env.BUILD_TAG} on ${env.JENKINS_URL}"
            git 'https://github.com/enrikiko/raspberry'
            sh("bash install_docker.sh ${IP}")
         }
      }
   }
   post {
     always {
         cleanWs()
     }
   }
}
